{
  "name": "@faims-project/faims3-map-input",
  "version": "1.2.0",
  "description": "Map based input widget for FAIMS3",
  "author": "stevecassidy",
  "license": "MIT",
  "repository": "FAIMS/faims3-map-input",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.modern.js",
  "source": "./src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rm -rf build/*; microbundle --jsx React.createElement --no-compress --format modern,cjs",
    "start": "microbundle watch --jsx React.createElement --no-compress --format modern,cjs",
    "rollup": "rollup -c",
    "test": "cross-env CI=1 react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "test:watch": "react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "yarn run build",
    "pretest": "yarn run build",
    "posttest": "yarn run lint"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "gts": "^3.1.1",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@capacitor/core": "5.0.3",
    "@capacitor/geolocation": "^5.0.2",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "5.13.2",
    "@testing-library/dom": ">=9",
    "@types/proj4": "^2.5.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "formik": "^2.2.9",
    "ol": "^7.3.0",
    "proj4": "^2.9.0"
  }
}
